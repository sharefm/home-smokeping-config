+ FPing6
binary = /usr/sbin/fping
protocol = 6

+ DNSdig
binary = /usr/bin/dig
forks = 5
offset = 50%
lookup = google.com
pings = 5
step = 30
timeout = 15
lookup = www.example.org
pings = 5
server = ns1.paltel.net



+ TCPPing
binary = /usr/bin/tcpping
forks = 10
offset = random
pings = 5
